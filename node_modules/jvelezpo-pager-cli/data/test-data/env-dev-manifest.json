{
    "global": {
        "affinity": {
            "nodeAffinity": {
                "requiredDuringSchedulingIgnoredDuringExecution": {
                    "nodeSelectorTerms": [
                        {
                            "matchExpressions": [
                                {
                                    "key": "env",
                                    "operator": "In",
                                    "values": [
                                        "dev"
                                    ]
                                }
                            ]
                        }
                    ]
                }
            }
        },
        "tolerations": [
            {
                "key": "dedicated",
                "value": "dev"
            }
        ],
        "imagePullSecrets": [
            {
                "name": "dev-pagerinc-docker-registry"
            }
        ]
    },
    "kong": {
        "env": {
            "pg_user": "kong",
            "pg_database": "kong",
            "pg_host": "dev-postgres-kong"
        },
        "proxy": {
            "ingress": {
                "enabled": true,
                "tls": [
                    {
                        "secretName": "pagerinc.com",
                        "hosts": [
                            "pgr-qa-api-auth.pagerinc.com",
                            "pgr-qa-api-admin.pagerinc.com",
                            "pgr-qa-api-cc.pagerinc.com",
                            "pgr-qa-api.pagerinc.com",
                            "pgr-qa-config.pagerinc.com",
                            "pgr-qa-edge-api-admin.pagerinc.com",
                            "pgr-qa-providers.pagerinc.com",
                            "pgr-qa-api-scheduling.pagerinc.com",
                            "pgr-qa-stash.pagerinc.com",
                            "pgr-qa-webhooks.pagerinc.com"
                        ]
                    }
                ],
                "hosts": [
                    "pgr-qa-api-auth.pagerinc.com",
                    "pgr-qa-api-admin.pagerinc.com",
                    "pgr-qa-api-cc.pagerinc.com",
                    "pgr-qa-api.pagerinc.com",
                    "pgr-qa-config.pagerinc.com",
                    "pgr-qa-edge-api-admin.pagerinc.com",
                    "pgr-qa-providers.pagerinc.com",
                    "pgr-qa-api-scheduling.pagerinc.com",
                    "pgr-qa-stash.pagerinc.com",
                    "pgr-qa-webhooks.pagerinc.com"
                ],
                "annotations": {
                    "kubernetes.io/ingress.class": "nginx-dev",
                    "nginx.ingress.kubernetes.io/proxy-read-timeout": "7200",
                    "nginx.ingress.kubernetes.io/proxy-send-timeout": "7200"
                }
            }
        }
    },
    "konfig": {
        "cors": "http://localhost:5000, https://pgr-qa-enterprise-admin.pagerinc.com",
        "domainPrefix": "pgr-qa"
    },
    "tags": {
        "admin": true,
        "auth": true,
        "cambia": true,
        "telemedicine": true
    },
    "api-auth": {
        "image": {
            "tag": "f3218d7"
        },
        "cognito": {
            "userPoolDomain": "https://pager-test.auth.us-east-2.amazoncognito.com"
        },
        "postgresql": {
            "enabled": true
        }
    },
    "api-command-center": {
        "image": {
            "tag": "15041c3"
        },
        "seed": {
            "enabled": true,
