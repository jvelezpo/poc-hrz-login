{
    "global": {
        "affinity": {
            "nodeAffinity": {
                "requiredDuringSchedulingIgnoredDuringExecution": {
                    "nodeSelectorTerms": [
                        {
                            "matchExpressions": [
                                {
                                    "key": "env",
                                    "operator": "In",
                                    "values": [
                                        "staging"
                                    ]
                                }
                            ]
                        }
                    ]
                }
            }
        },
        "tolerations": [
            {
                "key": "dedicated",
                "value": "staging"
            }
        ],
        "imagePullSecrets": [
            {
                "name": "staging-pagerinc-docker-registry"
            }
        ]
    },
    "kong": {
        "env": {
            "pg_user": "kong",
            "pg_database": "kong",
            "pg_host": "staging-postgres-kong"
        },
        "proxy": {
            "ingress": {
                "enabled": true,
                "tls": [
                    {
                        "secretName": "pagerinc.com",
                        "hosts": [
                            "pgr-stage-api-auth.pagerinc.com",
                            "pgr-stage-api-admin.pagerinc.com",
                            "pgr-stage-api-cc.pagerinc.com",
                            "pgr-stage-api.pagerinc.com",
                            "pgr-stage-config.pagerinc.com",
                            "pgr-stage-edge-api-admin.pagerinc.com",
                            "pgr-stage-providers.pagerinc.com",
                            "pgr-stage-api-scheduling.pagerinc.com",
                            "pgr-stage-stash.pagerinc.com",
                            "pgr-stage-webhooks.pagerinc.com"
                        ]
                    }
                ],
                "hosts": [
                    "pgr-stage-api-auth.pagerinc.com",
                    "pgr-stage-api-admin.pagerinc.com",
                    "pgr-stage-api-cc.pagerinc.com",
                    "pgr-stage-api.pagerinc.com",
                    "pgr-stage-config.pagerinc.com",
                    "pgr-stage-edge-api-admin.pagerinc.com",
                    "pgr-stage-providers.pagerinc.com",
                    "pgr-stage-api-scheduling.pagerinc.com",
                    "pgr-stage-stash.pagerinc.com",
                    "pgr-stage-webhooks.pagerinc.com"
                ],
                "annotations": {
                    "kubernetes.io/ingress.class": "nginx-staging",
                    "nginx.ingress.kubernetes.io/proxy-read-timeout": "7200",
                    "nginx.ingress.kubernetes.io/proxy-send-timeout": "7200"
                }
            }
        }
    },
    "konfig": {
        "domainPrefix": "pgr-stage",
        "cors": "http://localhost:5000, https://pgr-stage-enterprise-admin.pagerinc.com"
    },
    "tags": {
        "admin": true,
        "auth": true,
        "cambia": true
    },
    "migrations": {
        "seeds": true,
        "users": false
    },
    "api-command-center": {
        "image": {
            "tag": "56a0c21"
        }
    },
    "api-messages": {
        "image": {
            "tag": "85e2c9f"
        }
    },
    "api-demographics": {
        "image": {
            "tag": "6a2c65a"
        }
    },
    "api-providers": {
        "image": {
            "tag": "7d979a9"
        }
    },
    "api-scheduling": {
        "image": {
            "tag": "30499d8"
        }
    },
    "api-twilio": {
        "twilio": {
            "callURL": "https://pgr-stage-webhooks.pagerinc.com/voicecall",
            "fromNumber": "+17328203571"
        }
    },
    "api-users": {
        "image": {
            "tag": "3c72447"
        }
    },
    "api-auth": {
        "cognito": {
            "userPoolDomain": "https://pager-test.auth.us-east-2.amazoncognito.com"
        },
        "postgresql": {
            "enabled": true
        }
    },
    "chappie": {
        "enabled": true,
        "migrations": {
            "enabled": false,
            "uploadForAllOrgs": true
        },
        "image": {
            "tag": "1a651e7"
        }
    },
    "edge-api-public": {
        "image": {
            "tag": "3f49a07"
        }
    },
    "stash": {
        "stashURL": "https://pgr-stage-stash.pagerinc.com",
        "s3BucketName": "pgr-stage-uploads.pagerinc.com"
    },
    "tenant-middleware-cambia": {
        "image": {
            "tag": "4c9e926"
        },
        "keys": {
            "url": "http://localhost:3000/publicKeys"
        }
    },
    "redox": {
        "image": {
            "tag": "5aac504"
        }
    }
}