{
    "global": {
        "affinity": {
            "nodeAffinity": {
                "requiredDuringSchedulingIgnoredDuringExecution": {
                    "nodeSelectorTerms": [
                        {
                            "matchExpressions": [
                                {
                                    "key": "env",
                                    "operator": "In",
                                    "values": [
                                        "dev"
                                    ]
                                }
                            ]
                        }
                    ]
                }
            }
        },
        "tolerations": [
            {
                "key": "dedicated",
                "value": "dev"
            }
        ],
        "imagePullSecrets": [
            {
                "name": "hrz-qa-pagerinc-docker-registry"
            }
        ]
    },
    "kong": {
        "env": {
            "pg_user": "kong",
            "pg_database": "kong",
            "pg_host": "hrz-qa-postgres-kong"
        },
        "proxy": {
            "ingress": {
                "enabled": true,
                "tls": [
                    {
                        "secretName": "pagerinc.com",
                        "hosts": [
                            "hrz-qa-api-admin.pagerinc.com",
                            "hrz-qa-api-auth.pagerinc.com",
                            "hrz-qa-api-cc.pagerinc.com",
                            "hrz-qa-api-horizon.pagerinc.com",
                            "hrz-qa-api-scheduling.pagerinc.com",
                            "hrz-qa-api.pagerinc.com",
                            "hrz-qa-config.pagerinc.com",
                            "hrz-qa-edge-api-admin.pagerinc.com",
                            "hrz-qa-providers.pagerinc.com",
                            "hrz-qa-stash.pagerinc.com",
                            "hrz-qa-webhooks.pagerinc.com"
                        ]
                    }
                ],
                "hosts": [
                    "hrz-qa-api-admin.pagerinc.com",
                    "hrz-qa-api-auth.pagerinc.com",
                    "hrz-qa-api-cc.pagerinc.com",
                    "hrz-qa-api-horizon.pagerinc.com",
                    "hrz-qa-api-scheduling.pagerinc.com",
                    "hrz-qa-api.pagerinc.com",
                    "hrz-qa-config.pagerinc.com",
                    "hrz-qa-edge-api-admin.pagerinc.com",
                    "hrz-qa-providers.pagerinc.com",
                    "hrz-qa-stash.pagerinc.com",
                    "hrz-qa-webhooks.pagerinc.com"
                ],
                "annotations": {
                    "kubernetes.io/ingress.class": "nginx-dev",
                    "nginx.ingress.kubernetes.io/proxy-read-timeout": "7200",
                    "nginx.ingress.kubernetes.io/proxy-send-timeout": "7200"
                }
            }
        }
    },
    "konfig": {
        "cors": "http://localhost:5000, https://hrz-qa-enterprise-admin.pagerinc.com"
    },
    "tags": {
